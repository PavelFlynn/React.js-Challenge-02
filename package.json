  ],
  "engines": {
    "node": ">=18.12.0 <19",
    "npm": ">=8.6.0"
  },
  "scripts": {
    "test": "run-s -l cleanup test:* build:*",
    "dev": "vite",
    "preview": "vite preview",
    "UT:develop": "jest --watch --maxWorkers=1",
