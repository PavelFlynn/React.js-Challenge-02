    "build:vite": "vite build",
    "test:static": "eslint src --ext=ts",
    "test:unit": "jest",
    "cleanup": "rimraf coverage/ dist/"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
